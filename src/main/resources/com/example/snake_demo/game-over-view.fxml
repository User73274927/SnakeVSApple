<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxWidth="350.0" minHeight="350.0" prefHeight="350.0" prefWidth="350.0" spacing="25.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.snake_demo.controller.GameOverController">
    <Label text="Игра Окончена" underline="true">
      <font>
         <Font size="35.0" />
      </font>
    </Label>
    <Label text="0" fx:id="resultLabel">
      <font>
         <Font size="25.0" />
      </font>
    </Label>
   <Button text="Выйти" onAction="#OnExitPressed"/>
</VBox>
